{
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/asset-manifest.json",
      "dest": "asset-manifest.json"
    },
    {
      "src": "/logo.svg",
      "dest": "logo.svg"
    },
    {
      "src": "/logo192.png",
      "dest": "logo192.png"
    },
    {
      "src": "/logo512.png",
      "dest": "logo512.png"
    },
    {
      "src": "/manifest.json",
      "dest": "manifest.json"
    },
    {
      "src": "/robots.txt",
      "dest": "robots.txt"
    },
    {
      "src": "/favicon.ico",
      "dest": "favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}